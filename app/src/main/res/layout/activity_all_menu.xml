<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <import type="android.view.View"/>
        <import type="android.text.TextUtils"/>
        <import type="com.yesjnet.gwanak.core.EnumApp"/>
        <variable name="activity" type="com.yesjnet.gwanak.ui.main.AllMenuActivity"/>
        <variable
            name="viewModel"
            type="com.yesjnet.gwanak.ui.main.AllMenuViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clHeader"
            android:layout_width="match_parent"
            android:layout_height="72dp"
            android:background="@color/primary_color"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/tvLoginInfo"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="16dp"
                android:textColor="@color/white"
                android:textSize="14dp"
                android:fontFamily="@font/style_regular"
                app:bindLoginInfo="@{viewModel.onMemberInfo}"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/clLogin"/>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clLogin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/ivClose">
                <TextView
                    android:id="@+id/tvLogin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingVertical="6dp"
                    android:paddingHorizontal="16dp"
                    android:text="@string/login"
                    android:textColor="@color/primary_color"
                    android:textSize="14dp"
                    android:fontFamily="@font/noto_sans_kr_medium"
                    android:visibility="@{TextUtils.isEmpty(viewModel.onMemberInfo.userId) ? View.VISIBLE : View.GONE}"
                    android:onClick="@{() -> viewModel.onClickLogin()}"
                    cornerAll="@{16}"
                    bgColor="@{@color/white}"
                    rippleColor="@{@color/ripple_effect}"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"/>

                <ImageView
                    android:id="@+id/ivMyPage"
                    android:layout_width="34dp"
                    android:layout_height="34dp"
                    android:src="@drawable/btn_menu_my"
                    android:visibility="@{TextUtils.isEmpty(viewModel.onMemberInfo.userId) ? View.GONE : View.VISIBLE}"
                    android:onClick="@{() -> viewModel.onClickMyPage(viewModel.onMemberInfo)}"
                    rippleColor="@{@color/ripple_effect}"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <ImageView
                android:id="@+id/ivClose"
                android:layout_width="34dp"
                android:layout_height="34dp"
                android:layout_marginEnd="12dp"
                android:src="@drawable/btn_menu_close"
                android:onClick="@{() -> viewModel.onClickFinish()}"
                rippleColor="@{@color/ripple_effect}"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.core.widget.NestedScrollView
            android:id="@+id/nsvMain"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/clHeader"
            app:layout_constraintBottom_toBottomOf="parent">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingHorizontal="16dp"
                android:paddingVertical="30dp">
                <!-- 자료 검색 -->
                <TextView
                    android:id="@+id/tvDataSearchLabel"
                    style="@style/LabelText"
                    android:text="@string/data_search"
                    android:textColor="@color/color_111111"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"/>
                <include
                    android:id="@+id/icResourceSearch"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="6dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.RESOURCE_SEARCH)}"
                    app:value="@{@string/integrated_resource_search}"
                    app:menuDrawable="@{@drawable/icon_menu_a01}"
                    app:layout_constraintTop_toBottomOf="@+id/tvDataSearchLabel"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/icSearchNewInfo"/>
                <include
                    android:id="@+id/icSearchNewInfo"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.SEARCH_NEW_INFO)}"
                    app:value="@{@string/search_new_info}"
                    app:menuDrawable="@{@drawable/icon_menu_a02}"
                    app:layout_constraintTop_toTopOf="@id/icResourceSearch"
                    app:layout_constraintStart_toEndOf="@id/icResourceSearch"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <include
                    android:id="@+id/icULibrarySearch"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.U_LIBRARY_SEARCH)}"
                    app:value="@{@string/u_library_search}"
                    app:menuDrawable="@{@drawable/icon_menu_a03}"
                    app:layout_constraintTop_toBottomOf="@+id/icResourceSearch"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/icSmartLibrarySearch"/>
                <include
                    android:id="@+id/icSmartLibrarySearch"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.SMART_LIBRARY_SEARCH)}"
                    app:value="@{@string/smart_library_search}"
                    app:menuDrawable="@{@drawable/icon_menu_a04}"
                    app:layout_constraintTop_toTopOf="@id/icULibrarySearch"
                    app:layout_constraintStart_toEndOf="@id/icULibrarySearch"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <include
                    android:id="@+id/icFrequentlyReadBooks"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.FREQUENTLY_READ_BOOKS)}"
                    app:value="@{@string/frequently_read_books}"
                    app:menuDrawable="@{@drawable/icon_menu_a05}"
                    app:layout_constraintTop_toBottomOf="@+id/icULibrarySearch"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/vDataSearch"/>
                <View
                    android:id="@+id/vDataSearch"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    app:layout_constraintTop_toTopOf="@id/icFrequentlyReadBooks"
                    app:layout_constraintStart_toEndOf="@id/icFrequentlyReadBooks"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <View
                    android:id="@+id/vDataSearchLine"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="16dp"
                    android:background="@color/color_dddddd"
                    app:layout_constraintTop_toBottomOf="@id/icFrequentlyReadBooks"/>

                <!-- 이용안내 -->
                <TextView
                    android:id="@+id/tvUserGuide"
                    style="@style/LabelText"
                    android:layout_marginTop="19dp"
                    android:text="@string/user_guide"
                    android:textColor="@color/color_111111"
                    app:layout_constraintTop_toBottomOf="@id/vDataSearchLine"
                    app:layout_constraintStart_toStartOf="parent"/>
                <include
                    android:id="@+id/icNotice"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="6dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.NOTICE)}"
                    app:value="@{@string/notice}"
                    app:menuDrawable="@{@drawable/icon_menu_b01}"
                    app:layout_constraintTop_toBottomOf="@+id/tvUserGuide"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/icFacilityInfo"/>
                <include
                    android:id="@+id/icFacilityInfo"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.FACILITY_INFO)}"
                    app:value="@{@string/facility_info}"
                    app:menuDrawable="@{@drawable/icon_menu_b02}"
                    app:layout_constraintTop_toTopOf="@id/icNotice"
                    app:layout_constraintStart_toEndOf="@id/icNotice"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <View
                    android:id="@+id/vUserGuideLine"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="16dp"
                    android:background="@color/color_dddddd"
                    app:layout_constraintTop_toBottomOf="@id/icNotice"/>

                <!-- 신청/참여 -->
                <TextView
                    android:id="@+id/tvApplyParticipate"
                    style="@style/LabelText"
                    android:layout_marginTop="19dp"
                    android:text="@string/apply_participate"
                    android:textColor="@color/color_111111"
                    app:layout_constraintTop_toBottomOf="@id/vUserGuideLine"
                    app:layout_constraintStart_toStartOf="parent"/>
                <include
                    android:id="@+id/icReadingCultureProgram"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="6dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.READING_CULTURE_PROGRAM)}"
                    app:value="@{@string/reading_culture_program}"
                    app:menuDrawable="@{@drawable/icon_menu_c01}"
                    app:layout_constraintTop_toBottomOf="@+id/tvApplyParticipate"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/icRequestDesiredBook"/>
                <include
                    android:id="@+id/icRequestDesiredBook"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.REQUEST_DESIRED_BOOK)}"
                    app:value="@{@string/request_desired_book}"
                    app:menuDrawable="@{@drawable/icon_menu_c02}"
                    app:layout_constraintTop_toTopOf="@id/icReadingCultureProgram"
                    app:layout_constraintStart_toEndOf="@id/icReadingCultureProgram"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <include
                    android:id="@+id/icFreeDelivery"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.FREE_DELIVERY)}"
                    app:value="@{@string/free_delivery}"
                    app:menuDrawable="@{@drawable/icon_menu_c03}"
                    app:layout_constraintTop_toBottomOf="@+id/icRequestDesiredBook"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/icLoanFromLocalBookstores"/>
                <include
                    android:id="@+id/icLoanFromLocalBookstores"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.LOAN_FROM_LOCAL_BOOKSTORES)}"
                    app:value="@{@string/loan_from_local_bookstores}"
                    app:menuDrawable="@{@drawable/icon_menu_c04}"
                    app:layout_constraintTop_toTopOf="@id/icFreeDelivery"
                    app:layout_constraintStart_toEndOf="@id/icFreeDelivery"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <View
                    android:id="@+id/vApplyParticipateLine"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="16dp"
                    android:background="@color/color_dddddd"
                    app:layout_constraintTop_toBottomOf="@id/icLoanFromLocalBookstores"/>

                <!-- 전자도서관 -->
                <TextView
                    android:id="@+id/tvElectronicLibrary"
                    style="@style/LabelText"
                    android:layout_marginTop="19dp"
                    android:text="@string/electronic_library"
                    android:textColor="@color/color_111111"
                    app:layout_constraintTop_toBottomOf="@id/vApplyParticipateLine"
                    app:layout_constraintStart_toStartOf="parent"/>
                <include
                    android:id="@+id/icEBooks"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="6dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.E_BOOKS)}"
                    app:value="@{@string/e_books}"
                    app:menuDrawable="@{@drawable/icon_menu_d01}"
                    app:layout_constraintTop_toBottomOf="@+id/tvElectronicLibrary"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/icAudioBooks"/>
                <include
                    android:id="@+id/icAudioBooks"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.AUDIO_BOOKS)}"
                    app:value="@{@string/audio_books}"
                    app:menuDrawable="@{@drawable/icon_menu_d02}"
                    app:layout_constraintTop_toTopOf="@id/icEBooks"
                    app:layout_constraintStart_toEndOf="@id/icEBooks"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <View
                    android:id="@+id/vElectronicLibraryLine"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="16dp"
                    android:background="@color/color_dddddd"
                    app:layout_constraintTop_toBottomOf="@id/icEBooks"/>

                <!-- 나의도서관 -->
                <TextView
                    android:id="@+id/tvMyLibrary"
                    style="@style/LabelText"
                    android:layout_marginTop="19dp"
                    android:text="@string/my_library"
                    android:textColor="@color/color_111111"
                    app:layout_constraintTop_toBottomOf="@id/vElectronicLibraryLine"
                    app:layout_constraintStart_toStartOf="parent"/>
                <include
                    android:id="@+id/icMyInfo"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="6dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.MY_INFO)}"
                    app:value="@{@string/my_info}"
                    app:menuDrawable="@{@drawable/icon_menu_e01}"
                    app:layout_constraintTop_toBottomOf="@+id/tvMyLibrary"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/icBookUsageHistory"/>
                <include
                    android:id="@+id/icBookUsageHistory"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.BOOK_USAGE_HISTORY)}"
                    app:value="@{@string/book_usage_history}"
                    app:menuDrawable="@{@drawable/icon_menu_e02}"
                    app:layout_constraintTop_toTopOf="@id/icMyInfo"
                    app:layout_constraintStart_toEndOf="@id/icMyInfo"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <include
                    android:id="@+id/icInterlibraryLoanRequestHistory"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.INTERLIBRARY_LOAN_REQUEST_HISTORY)}"
                    app:value="@{@string/interlibrary_loan_request_history}"
                    app:menuDrawable="@{@drawable/icon_menu_e03}"
                    app:layout_constraintTop_toBottomOf="@+id/icBookUsageHistory"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/icMyBooksOfInterest"/>
                <include
                    android:id="@+id/icMyBooksOfInterest"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.MY_BOOKS_OF_INTEREST)}"
                    app:value="@{@string/my_books_of_interest}"
                    app:menuDrawable="@{@drawable/icon_menu_e04}"
                    app:layout_constraintTop_toTopOf="@id/icInterlibraryLoanRequestHistory"
                    app:layout_constraintStart_toEndOf="@id/icInterlibraryLoanRequestHistory"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <include
                    android:id="@+id/icMyRequestHistory"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.MY_REQUEST_HISTORY)}"
                    app:value="@{@string/my_request_history}"
                    app:menuDrawable="@{@drawable/icon_menu_e05}"
                    app:layout_constraintTop_toBottomOf="@+id/icMyBooksOfInterest"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/icRequestForDesiredBooks"/>
                <include
                    android:id="@+id/icRequestForDesiredBooks"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.REQUEST_FOR_DESIRED_BOOKS)}"
                    app:value="@{@string/request_for_desired_books}"
                    app:menuDrawable="@{@drawable/icon_menu_e06}"
                    app:layout_constraintTop_toTopOf="@id/icMyRequestHistory"
                    app:layout_constraintStart_toEndOf="@id/icMyRequestHistory"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <View
                    android:id="@+id/vMyLibraryLine"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="16dp"
                    android:background="@color/color_dddddd"
                    app:layout_constraintTop_toBottomOf="@id/icMyRequestHistory"/>

                <!-- 회원정보 -->
                <TextView
                    android:id="@+id/tvMembershipInformation"
                    style="@style/LabelText"
                    android:layout_marginTop="19dp"
                    android:text="@string/membership_information"
                    android:textColor="@color/color_111111"
                    app:layout_constraintTop_toBottomOf="@id/vMyLibraryLine"
                    app:layout_constraintStart_toStartOf="parent"/>
                <include
                    android:id="@+id/icLogin"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="6dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.LOGIN)}"
                    app:value="@{@string/login}"
                    app:menuDrawable="@{@drawable/icon_menu_f01}"
                    app:layout_constraintTop_toBottomOf="@+id/tvMembershipInformation"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/icMobileMembershipCard"/>
                <include
                    android:id="@+id/icMobileMembershipCard"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.MOBILE_MEMBERSHIP_CARD)}"
                    app:value="@{@string/mobile_membership_card}"
                    app:menuDrawable="@{@drawable/icon_menu_f02}"
                    app:layout_constraintTop_toTopOf="@id/icLogin"
                    app:layout_constraintStart_toEndOf="@id/icLogin"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <include
                    android:id="@+id/icEditMembershipInformation"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.EDIT_MEMBERSHIP_INFOMATION)}"
                    app:value="@{@string/edit_membership_information}"
                    app:menuDrawable="@{@drawable/icon_menu_f03}"
                    app:layout_constraintTop_toBottomOf="@+id/icLogin"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/icChangePassword"/>
                <include
                    android:id="@+id/icChangePassword"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.CHANGE_PASSWORD)}"
                    app:value="@{@string/change_password}"
                    app:menuDrawable="@{@drawable/icon_menu_f04}"
                    app:layout_constraintTop_toTopOf="@id/icEditMembershipInformation"
                    app:layout_constraintStart_toEndOf="@id/icEditMembershipInformation"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <include
                    android:id="@+id/icReAgreePersonalInformation"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.RE_AGREE_PERSONAL_INFOMATION)}"
                    app:value="@{@string/re_agree_personal_information}"
                    app:menuDrawable="@{@drawable/icon_menu_f05}"
                    app:layout_constraintTop_toBottomOf="@+id/icEditMembershipInformation"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/vDataSearch"/>
                <View
                    android:id="@+id/vMemberShipInfo"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    app:layout_constraintTop_toTopOf="@id/icReAgreePersonalInformation"
                    app:layout_constraintStart_toEndOf="@id/icReAgreePersonalInformation"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <View
                    android:id="@+id/vMemberShipInfoLine"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="16dp"
                    android:background="@color/color_dddddd"
                    app:layout_constraintTop_toBottomOf="@id/icReAgreePersonalInformation"/>

                <!-- 설정 -->
                <TextView
                    android:id="@+id/tvSettings"
                    style="@style/LabelText"
                    android:layout_marginTop="19dp"
                    android:text="@string/settings"
                    android:textColor="@color/color_111111"
                    app:layout_constraintTop_toBottomOf="@id/vMemberShipInfoLine"
                    app:layout_constraintStart_toStartOf="parent"/>
                <include
                    android:id="@+id/icAppSettings"
                    layout="@layout/layout_all_menu"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="6dp"
                    android:onClick="@{() -> viewModel.onClickMenu(EnumApp.WebType.APP_SETTINGS)}"
                    app:value="@{@string/app_settings}"
                    app:menuDrawable="@{@drawable/icon_menu_g01}"
                    app:layout_constraintTop_toBottomOf="@+id/tvSettings"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/vSettings"/>
                <View
                    android:id="@+id/vSettings"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    app:layout_constraintTop_toTopOf="@id/icAppSettings"
                    app:layout_constraintStart_toEndOf="@id/icAppSettings"
                    app:layout_constraintEnd_toEndOf="parent"/>

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.core.widget.NestedScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>