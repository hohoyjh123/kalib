<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="2dp"
        android:right="2dp"
        android:top="2dp"
        android:bottom="2dp">

        <shape
            android:shape="oval">

            <size
                android:width="26dp"
                android:height="26dp" />

            <solid
                android:color="@color/white" />

            <stroke
                android:width="1dp"
                android:color="@color/white" />

        </shape>
    </item>
</layer-list>