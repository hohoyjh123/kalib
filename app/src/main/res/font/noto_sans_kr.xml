<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute">

    <!-- Regular -->
    <font
        android:font="@font/notosanskr_regular_hestia"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/notosanskr_regular_hestia"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- Medium -->
    <font
        android:font="@font/noto_sans_kr_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/noto_sans_kr_medium"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <!-- Bold -->
    <font
        android:font="@font/notosanskr_bold_hestia"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/notosanskr_bold_hestia"
        app:fontStyle="normal"
        app:fontWeight="700" />

</font-family>